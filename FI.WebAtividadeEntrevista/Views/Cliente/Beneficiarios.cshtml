<form id="formBeneficiario" method="post">
    <div class="row">
        <div class="col-md-5">
            <div class="form-group">
                <label for="CPF">CPF:</label>
                <input required="required" type="text" class="form-control" id="CPFBeneficiario" name="CPF" placeholder="Ex.: 111.111.111-11" maxlength="50">
            </div>
        </div>
        <div class="col-md-5">
            <div class="form-group">
                <label for="Nome">Nome:</label>
                <input required="required" type="text" class="form-control" id="NomeBeneficiario" name="Nome" placeholder="Ex.: Maria da Silva" maxlength="255">
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label class="invisible">Incluir</label>
                <button type="button" id="incBeneficiario" class="btn btn-sm btn-success d-block mx-auto">Incluir</button>
            </div>
        </div>
    </div>
</form>

<table id="gridBeneficiarios" class="table"></table>